<% if (index) { %>
    <% if (post.link) { %>
        <a class="<%= class_name %>" href="<%- url_for(post.link) %>" target="_blank" itemprop="url">
            <% if (post.icon) { %>
                <i class="<%= post.icon %>"></i>
            <% } %>
            <%= post.title %>
        </a>
    <% } else if (post.title) { %>
        <a class="<%= class_name %>" href="<%- url_for(post.path) %>">
            <% if (post.icon) { %>
                <i class="<%= post.icon %>"></i>
            <% } %>
            <%= post.title %>
        </a>
    <% } else { %>
        <a class="<%= class_name %>" href="<%- url_for(post.path) %>">Untitled</a>
    <% } %>
<% } else { %>
    <h1 class="<%= class_name %> p-name" itemprop="name headline">
        <% if (post.icon) { %>
            <i class="<%= post.icon %>"></i>
        <% } %>
        <%= post.title %>
    </h1>
<% } %>

